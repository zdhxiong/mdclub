    "@babel/plugin-transform-react-jsx": "^7.10.4",
    "@babel/preset-env": "^7.11.5",
    "@hyperapp/logger": "^0.5.0",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.1.0",
